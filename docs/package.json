    "@iconify-json/noto": "^1.2.1",
    "@iconify-json/ph": "^1.2.1",
    "@iconify-json/radix-icons": "^1.2.1",
    "@iconify-json/simple-icons": "^1.2.8",
    "@iconify-json/uil": "^1.2.1",
    "@iconify-json/vscode-icons": "^1.2.2",
    "@inspira-ui/plugins": "^0.0.1",
    "@nuxt/content": "3.0.0-alpha.1",
    "@nuxt/devtools": "1.6.0",
    "@nuxt/fonts": "^0.10.0",
    "@nuxt/image": "^1.8.1",
