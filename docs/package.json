    "nuxt-build-cache": "^0.1.1",
    "nuxt-content-twoslash": "^0.1.1",
    "nuxt-lodash": "^2.5.3",
    "ofetch": "1.4.1",
    "radix-vue": "^1.9.7",
    "shiki": "1.22.0",
